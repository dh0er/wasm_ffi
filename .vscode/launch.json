{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Example",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "cwd": "example",
    },
    {
      "name": "Dart Native example",
      "type": "dart",
      "request": "launch",
      "program": "bin/example.dart",
      "cwd": "example_dart",
    },
    {
      "name": "Dart Web example",
      "type": "dart",
      "request": "launch",
      "program": "web",
      "cwd": "example_dart",
    },
    {
      "name": "Bump Version",
      "type": "dart",
      "request": "launch",
      "program": "tool/bump_version.dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "patch",
        "log",
        "test",
      ]
    },
  ]
}